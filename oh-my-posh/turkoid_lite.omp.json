{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ffc62f",
          "background": "transparent",
          "properties": {
            "prefix": " ",
            "postfix": " <#aaaaaa>»</>",
            "root_icon": "\uE0A2"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#81c784",
          "background": "transparent",
          "properties": {
            "prefix": " ",
            "postfix": " "
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#56b6c2",
          "background": "transparent",
          "properties": {
            "prefix": "<#aaaaaa>»</> ",
            "postfix": " ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#d0666f",
          "background": "transparent",
          "properties": {
            "prefix": "<#aaaaaa>»</> ",
            "postfix": " ",
            "display_branch_status": true,
            "display_stash_count": true,
            "local_changes_color": "#ffeb3b",
            "working_color": "#ff1744",
            "staging_color": "#4caf50",
            "ahead_color": "#26c6da",
            "behind_color": "#ff1744",
            "local_working_icon": "",
            "local_staged_icon": "",
            "stash_count_icon": ""
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#4b8bbe",
          "background": "transparent",
          "properties": {
            "prefix": "<#aaaaaa>»</> ",
            "postfix": "",
            "display_mode": "environment"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "\u276F"
          }
        }
      ]
    }
  ]
}