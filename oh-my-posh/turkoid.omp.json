{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "mint": "<#a2e4b8>\uf30f</>",
            "ubuntu": "<#e95420>\uf31c</>",
            "windows": "<#00adef>\ue70f</>"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#aaaaaa",
          "background": "#2f2f2f",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf0e7"
          },
          "template": "\uf0e7",
          "foreground": "#ffc62f",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#aaaaaa>\ue0b1</> {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }}",
          "foreground": "#81c784",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "<#aaaaaa>\ue0b1</> {{ .Path }}",
          "foreground": "#56b6c2",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "ahead_color": "#26c6da",
            "behind_color": "#ff1744",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "local_changes_color": "#ffeb3b"
          },
          "template": "<#aaaaaa>\ue0b1</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 <#ff1744>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 <#4caf50>{{ .Staging.String }}</>{{ end }}",
          "foreground": "#d0666f",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment"
          },
          "template": "<#aaaaaa>\ue0b1</> <#ffd43b>\ue235</> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#4b8bbe",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "template": "<#aaaaaa>\ue0b1</> <#ffd43b>\ufb0f</> {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#00adef",
          "powerline_symbol": "\ue0b0",
          "background": "#2f2f2f",
          "type": "dotnet",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
