{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "foreground": "#aaaaaa",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "\u2800",
            "postfix": "",
            "mint": "<#a2e4b8>\uF30F</>",
            "ubuntu": "<#e95420>\uF31C</>",
            "windows": "<#00adef>\uE70F</>"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffc62f",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "",
            "postfix": "",
            "root_icon": "\uF0E7"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#81c784",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "<#aaaaaa>\uE0B1</>\u2800",
            "postfix": ""
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#56b6c2",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "<#aaaaaa>\uE0B1</>\u2800",
            "postfix": "",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#d0666f",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "<#aaaaaa>\uE0B1</>\u2800",
            "postfix": "",
            "color_background": false,
            "display_stash_count": true,
            "local_changes_color": "#ffeb3b",
            "working_color": "#ff1744",
            "staging_color": "#4caf50",
            "ahead_color": "#26c6da",
            "behind_color": "#ff1744"
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#4b8bbe",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "<#aaaaaa>\uE0B1</> <#ffd43b>\uE235</>\u2800",
            "postfix": "",
            "display_mode": "environment"
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#00adef",
          "background": "#2f2f2f",
          "properties": {
            "prefix": "<#aaaaaa>\uE0B1</> <#ffd43b>\uFB0F</>\u2800",
            "postfix": "",
            "display_version": false
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "transparent",
          "properties": {
            "threshold": 500,
            "prefix": "",
            "postfix": "",
            "style": "roundrock"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#00c853",
          "background": "transparent",
          "properties": {
            "prefix": "\u2800",
            "postfix": "",
            "display_exit_code": false,
            "always_enabled": false,
            "error_color": "#e91e63",
            "color_background": false,
            "success_icon": "\u2714",
            "error_icon": "\u2718"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "\u276F"
          }
        }
      ]
    }
  ]
}